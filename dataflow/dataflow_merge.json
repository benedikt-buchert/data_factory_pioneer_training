{
	"name": "dataflow_merge",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "postgres",
						"type": "DatasetReference"
					},
					"name": "retail"
				},
				{
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT * FROM reail',",
				"     format: 'query') ~> retail",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source2"
			]
		}
	}
}